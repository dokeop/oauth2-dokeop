{
    "name": "dokeop/oauth2-dokeop",
    "description": "Dokeop OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Dokeop",
            "email": "tech@dokeop.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "dokeop"
    ],
    "require": {
        "php": "^5.6|^7.0",
        "league/oauth2-client": "^2.0"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check": "phpcs src --standard=psr2 -sp"
    }
}
